
fontFamily = 'Inconsolata','Source Code Pro', 'Courier New', arial, helvetica, sans-serif

.logEntry
  padding 10px 20px
  border-left 5px solid transparent
  background-color #fff
  margin-bottom 5px
  box-shadow 0 2px 2px rgba(0,0,0,0.1);

.message
  font-family fontFamily

.info
  border-left-color #62B5F6
.warn
  border-left-color #FFC107
.error
  border-left-color #D50000
